print(len(nltk.corpus.abc.words()))
print(nltk.corpus.abc.words()[:19])

number = 0
for i in nltk.corpus.abc.words():
    if i == 'pizza':
        number += 1
print(number)

def look():
    word = input('pick a word')
    place1 = 0
    for i,f in enumerate(nltk.corpus.abc.words()):
        if f == word:
            print(word)
            print(nltk.corpus.abc.words()[i+1])
            print(' ')
look()

